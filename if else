# include <stdio.h>
int main () {
    float average,total_marks;
    int subject1,subject2,subject3,subject4,subject5,subject6;
     
     printf("enter your marks of subject1: ");
    scanf("%d",&subject1);
     printf("enter your marks of subject2: ");
    scanf("%d",&subject2);
     printf("enter your marks of subject3: ");
    scanf("%d",&subject3);
     printf("enter your marks of subject4: ");
    scanf("%d",&subject4);
     printf("enter your marks of subject5: ");
    scanf("%d",&subject5);
     printf("enter your marks of subject6: ");
    scanf("%d",&subject6);
    
    total_marks = subject1+subject2+subject3+subject4+subject5+subject6;
    printf("Total_marks:%.2f\n",total_marks);
    average = total_marks/6;
     printf("average:%.2f\n",average);
      if (average >= 90) {
          printf("Grade:A");
      }
      else if (average >= 80) {
          printf("Grade:B");
      }
      else if (average >= 75) {
          printf("Grade:C");
      }
      else if (average >= 60) {
          printf("Grade:D");
      } else {
          printf("F");
      }
return 0;
    
    }
